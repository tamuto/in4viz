{
  "name": "@infodb/in4viz",
  "version": "0.0.1",
  "description": "Core library for Infrastructure visualization with SVG",
  "author": "tamuto <tamuto@infodb.jp>",
  "license": "MIT",
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tamuto/in4viz.git",
    "directory": "packages/core"
  },
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "clean": "rm -rf dist",
    "lint": "eslint src --ext .ts,.tsx",
    "lint:fix": "eslint src --ext .ts,.tsx --fix",
    "test": "jest",
    "test:watch": "jest --watch"
  },
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "@svgdotjs/svg.js": "^3.2.4"
  },
  "devDependencies": {
    "@types/jest": "^29.5.12",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.2"
  },
  "keywords": [
    "infrastructure",
    "visualization",
    "svg",
    "aws",
    "cloud",
    "diagram"
  ]
}
